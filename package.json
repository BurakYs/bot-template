{
  "name": "bot-template",
  "version": "1.0.0",
  "description": "A template for creating Discord bots with Discord.js",
  "main": "dist/index",
  "author": "BurakYs",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node .",
    "dev": "tsc && cross-env NODE_ENV=development node .",
    "register-commands": "node . --redeployCommands",
    "build": "tsc",
    "lint": "eslint ."
  },
  "dependencies": {
    "discord-api-types": "^0.37.101",
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "glob": "^11.0.0",
    "module-alias": "^2.2.3",
    "tslog": "^4.9.3",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.7.4",
    "@types/yargs": "^17.0.33",
    "cross-env": "^7.0.3",
    "eslint": "~9.12.0",
    "typescript-eslint": "^8.8.0"
  },
  "_moduleAliases": {
    "@": "./dist"
  }
}
