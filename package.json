{
  "name": "bot-template",
  "version": "1.0.0",
  "description": "A template for creating Discord bots with Discord.js",
  "main": "dist/index",
  "author": "BurakYs",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=production node .",
    "dev": "tsc && cross-env NODE_ENV=development node .",
    "register-commands": "cross-env NODE_ENV=production node ./dist/registerCommands.js",
    "build": "tsc",
    "lint": "eslint"
  },
  "dependencies": {
    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "glob": "^11.0.0",
    "module-alias": "^2.2.3",
    "tslog": "^4.9.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^22.7.4",
    "cross-env": "^7.0.3",
    "eslint": "~9.12.0",
    "typescript-eslint": "^8.8.0"
  },
  "_moduleAliases": {
    "@": "./dist"
  }
}
